{
  "name": "solid-a11y-dialog",
  "version": "0.2.1",
  "description": "Solid implementation of a11y-dialog to build lightweight yet flexible script to create accessible dialog windows",
  "bugs": "https://github.com/Matth10/solid-a11y-dialog/issues",
  "homepage": "https://github.com/Matth10/solid-a11y-dialog",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs,js",
      "import": "./dist/index.es.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Matth10/solid-a11y-dialog"
  },
  "keywords": [
    "modal",
    "dialog",
    "accessibile dialog",
    "accessibility",
    "a11y",
    "a11y-dialog",
    "solid-js",
    "component"
  ],
  "license": "MIT",
  "author": "Matth10",
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.ts,.tsx . --fix",
    "format": "prettier --write .",
    "postinstall": "node ./fix-jest-dom.mjs"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-solid": "^0.4.6",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-dts": "^1.0.5",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.9.0"
  },
  "dependencies": {
    "a11y-dialog": "^7.4.0",
    "solid-js": "^1.3.14"
  }
}
